{
  "name": "DWatch",
  "version": "1.0.0-alpha.1",
  "description": "Manage your docker containers, images and networks in a nice and clean way.",
  "main": "index.js",
  "author": {
    "name": "Benjamin Otto",
    "email": "benjamin.otto@mercateo.com"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://github.com/Mercateo/dwatch/blob/master/LICENSE.md"
    }
  ],
  "homepage": "https://github.com/Mercateo/dwatch",
  "scripts": {
    "postinstall": "typings install",
    "build": "webpack --config webpack-dev.config.js",
    "watch": "npm run build && webpack-dev-server --config webpack-dev.config.js",
    "start": "electron .",
    "build-release": "webpack --config webpack-prod.config.js",
    "dist-all": "npm run test && npm run build-release && build --platform=all --arch=x64",
    "dist": "build",
    "test": "webpack --config webpack-test.config.js && karma start && remap-istanbul -i generated/coverage.json > generated/coverage-final.json && node scripts/generate-coverage.js text",
    "coverage-html": "npm run test && node scripts/generate-coverage.js html",
    "coverage-lcovonly": "npm run test && node scripts/generate-coverage.js lcovonly"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mercateo/dwatch.git"
  },
  "productName": "DWatch",
  "build": {
    "app-bundle-id": "com.mercateo.dwatch",
    "app-category-type": "public.app-category.developer-tools",
    "iconUrl": "https://oc.style-systems.de/index.php/s/YzOZJiotM5guUmk"
  },
  "directories": {
    "buildResources": "assets"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "dockerode": "^2.2.10",
    "flag-icon-css": "^1.3.0",
    "inversify": "^2.0.0-beta.4",
    "inversify-binding-decorators": "^1.0.0-alpha.2",
    "material-design-lite": "^1.1.3",
    "mobx": "^2.1.1",
    "mobx-react": "^3.0.3",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-intl": "^2.0.0",
    "react-router": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "electron-builder": "^3.23.0",
    "electron-prebuilt": "^0.37.6",
    "emock": "0.0.11",
    "expect": "^1.20.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.12.0",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-electron": "^3.2.0",
    "karma-mocha": "^0.2.2",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mobx-react-devtools": "^4.0.2",
    "mocha": "^2.4.5",
    "null-loader": "^0.1.1",
    "remap-istanbul": "^0.6.3",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.9.0-dev.20160514",
    "typings": "^1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.0.0"
  }
}
